{
    "compilerOptions": {
        "outDir": "./bin/dist/",
        "target": "es5",
        "strictNullChecks": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "module": "es2015",
        "lib": ["es6","dom"],
        "types": ["reflect-metadata"],
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "moduleResolution": "node"
    }
}